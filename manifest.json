{
  "name": "Ingress Intel Enable (IITC for Chrome)",
  "description": "Launch IITC for Ingress Intel with ease.",
  "version": "0.2.1",
  "permissions": [
    "tabs", "activeTab", "cookies", "https://*.ingress.com/intel"
  ],
  "default_locale": "en",
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "page_action": {
    "default_title": "Toggle IITC on ingress.com/intel",
    "default_icon": {
      "48": "assets/images/48/logo.png"
    }
  },
  "icons": {
    "48" : "assets/images/48/logo.png",
    "128" : "assets/images/128/logo.png"
  },
  "manifest_version": 2
}
